        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Delete oldest logs" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.model.Filters

def collection = props.get(&quot;logs_collection&quot;)

def idsToDelete = collection.find().limit(2)
    .into(new ArrayList&lt;&gt;())
    .collect { it.getObjectId(&quot;_id&quot;) }

if (idsToDelete) {
    collection.deleteMany(Filters.in(&quot;_id&quot;, idsToDelete))
} else {
    println &quot;Nenhum documento encontrado para deletar.&quot;
}</stringProp>
        </JSR223Sampler>
