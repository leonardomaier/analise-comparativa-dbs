        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Group and count by log_level" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection;
import org.bson.Document;

MongoCollection&lt;Document&gt; collection = props.get(&quot;logs_collection&quot;);

// MongoDB aggregation pipeline to group by log_level and count occurrences
collection.aggregate([
    // Group by log_level and count the occurrences
    new Document(&apos;$group&apos;, new Document(&apos;_id&apos;, &apos;$log_level&apos;).append(&apos;log_count&apos;, new Document(&apos;$sum&apos;, 1))),
    
    // Sort by log_count in descending order
    new Document(&apos;$sort&apos;, new Document(&apos;log_count&apos;, -1))
]).into(new ArrayList&lt;&gt;());</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Group and count by log_level and %" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection
import org.bson.Document

MongoCollection&lt;Document&gt; collection = props.get(&quot;logs_collection&quot;)

def results = collection.aggregate([
    // 1. Match apenas os log_levels existentes (força o uso de índice em log_level, se houver)
    new Document(&quot;\$match&quot;, new Document(&quot;log_level&quot;, new Document(&quot;\$in&quot;, [&quot;INFO&quot;, &quot;ERROR&quot;, &quot;WARN&quot;, &quot;DEBUG&quot;]))),

    // 2. Agrupa por log_level e conta
    new Document(&quot;\$group&quot;, new Document(&quot;_id&quot;, &quot;\$log_level&quot;)
        .append(&quot;log_count&quot;, new Document(&quot;\$sum&quot;, 1))
    ),

    // 3. Usando $facet para calcular total e fazer o join lógico
    new Document(&quot;\$facet&quot;, [
        &quot;logsByLevel&quot;: [
            new Document(&quot;\$sort&quot;, new Document(&quot;log_count&quot;, -1)) // ordena dentro do facet
        ],
        &quot;total&quot;: [
            new Document(&quot;\$group&quot;, new Document(&quot;_id&quot;, null)
                .append(&quot;total_logs&quot;, new Document(&quot;\$sum&quot;, &quot;\$log_count&quot;))
            )
        ]
    ]),

    // 4. Junta o resultado do facet em único documento
    new Document(&quot;\$project&quot;, new Document(&quot;logsByLevel&quot;, &quot;\$logsByLevel&quot;)
        .append(&quot;total&quot;, new Document(&quot;\$arrayElemAt&quot;, [&quot;\$total&quot;, 0]))
    ),

    // 5. Desconstrói logsByLevel para aplicar a porcentagem
    new Document(&quot;\$unwind&quot;, &quot;\$logsByLevel&quot;),

    // 6. Projeta os campos finais com percentual
    new Document(&quot;\$project&quot;, new Document(&quot;log_level&quot;, &quot;\$logsByLevel._id&quot;)
        .append(&quot;log_count&quot;, &quot;\$logsByLevel.log_count&quot;)
        .append(&quot;percentage&quot;, new Document(&quot;\$round&quot;, [
            new Document(&quot;\$multiply&quot;, [
                new Document(&quot;\$divide&quot;, [&quot;\$logsByLevel.log_count&quot;, &quot;\$total.total_logs&quot;]),
                100
            ]),
            2
        ]))
    ),

    // 7. Ordena final por percentual
    new Document(&quot;\$sort&quot;, new Document(&quot;percentage&quot;, -1))

]).into(new ArrayList&lt;&gt;())

// (Opcional) log de sucesso
log.info(&quot;Aggregation with match and facet executed.&quot;)
</stringProp>
        </JSR223Sampler>
