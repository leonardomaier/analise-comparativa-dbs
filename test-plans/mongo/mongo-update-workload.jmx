        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Delete oldest logs">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.model.Filters
import org.bson.Document

def collection = props.get(&quot;logs_collection&quot;)

int threadId = ctx.getThreadNum()

def idsToDelete = collection.find(new Document(&quot;test_id&quot;, threadId))
    .limit(2)
    .into(new ArrayList&lt;&gt;())
    .collect { it.getObjectId(&quot;_id&quot;) }

if (idsToDelete) {
    collection.deleteMany(Filters.in(&quot;_id&quot;, idsToDelete))
}</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Update single random log">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.model.Updates;
import com.mongodb.client.MongoCollection;
import org.bson.Document;
import java.util.Random;

MongoCollection&lt;Document&gt; collection = props.get(&quot;logs_collection&quot;);
Random rand = new Random();

int threadId = ctx.getThreadNum()  // Get current thread ID

// Find the first 100 documents by natural order (insertion order)
List&lt;Document&gt; documents = collection.find(new Document(&quot;test_id&quot;, threadId))
    .limit(100)
    .into(new ArrayList&lt;&gt;())

// Loop over the documents and update them
for (Document doc : documents) {
    int randomValue = rand.nextInt(1000)
    collection.updateOne(
        new Document(&quot;_id&quot;, doc.get(&quot;_id&quot;)),
        Updates.set(&quot;message&quot;, &quot;Updated log message: &quot; + randomValue)
    )
}</stringProp>
        </JSR223Sampler>
