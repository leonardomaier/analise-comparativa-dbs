        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Group and count by log_level and %" enabled="true">
          <stringProp name="dataSource">jdbcConfig</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">WITH total AS (
  SELECT COUNT(*) AS total_logs
  FROM &quot;analise-comparativa-dbs&quot;.logs
)
SELECT log_level,
       COUNT(*) AS log_count,
       ROUND((COUNT(*) * 100.0 / total.total_logs), 2) AS percentage
FROM &quot;analise-comparativa-dbs&quot;.logs, total
GROUP BY log_level, total.total_logs
ORDER BY percentage DESC;</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="variableNames"></stringProp>
          <stringProp name="resultVariable"></stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="resultSetMaxRows"></stringProp>
          <stringProp name="resultSetHandler">Store as String</stringProp>
        </JDBCSampler>
