          <stringProp name="dataSource">jdbcConfig</stringProp>
          <stringProp name="query">INSERT INTO &quot;analise-comparativa-dbs&quot;.logs (timestamp, log_level, message, source, test_id)
SELECT
	timestamp &apos;2020-01-10 20:00:00&apos; +
       random() * (timestamp &apos;2020-01-20 20:00:00&apos; -
                   timestamp &apos;2024-01-10 10:00:00&apos;),                               
    &apos;${log_level}&apos;,
    &apos;Random log message: ${__Random(1,1000,)}&apos;,     
    &apos;Source: ${source}&apos;,             
    ${__Random(1,10,)}                          
FROM generate_series(1,10);
</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="queryType">Update Statement</stringProp>
          <stringProp name="resultVariable"></stringProp>
          <stringProp name="variableNames">col1</stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="resultSetMaxRows"></stringProp>
          <stringProp name="resultSetHandler">Store as String</stringProp>
        </JDBCSampler>
